<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.cs2340.spacetraders">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.TraderOfferActivity"></activity>
        <activity android:name=".views.TraderEncounterActivity" />
        <activity android:name=".views.PoliceEncounterActivity" />
        <activity android:name=".views.PirateEncounterActivity" />
        <activity android:name=".views.MarketplaceActivity" />
        <activity android:name=".views.ShipStatsActivity" />
        <activity android:name=".views.MainGame" />
        <activity android:name=".views.addNewPerson" />
        <activity android:name=".views.CurrentSS" />
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>